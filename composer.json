{
    "name": "huangdijia/laravel-ssdb",
    "description": "ssdb for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "ssdb"
    ],
    "homepage": "https://github.com/huangdijia/laravel-ssdb",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/contracts": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Huangdijia\\Ssdb\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\Ssdb\\SsdbServiceProvider"
            ]
        }
    }
}